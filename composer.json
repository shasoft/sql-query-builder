{
    "name": "shasoft/sql-query-builder",
    "type": "library",
    "description": "Package for generating SQL queries to the database",
    "license": "MIT",
    "require": {
        "shasoft/db-schema": "*",
        "psr/cache": "^3"
    },
    "autoload": {
        "psr-4": {
            "Shasoft\\SqlQueryBuilder\\": "classes"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "10.3.5",
        "robiningelbrecht/phpunit-pretty-print": "^1.2",
        "shasoft/dump": "*",
        "shasoft/db-tool": "*",
        "shasoft/db-schema-dev": "*",
        "shasoft/psr-cache": "*"
    },
    "autoload-dev": {
        "psr-4": {
            "Shasoft\\SqlQueryBuilder\\Tests\\": "tests/classes"
        }
    },
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Shabanov Valera",
            "email": "shasoft_sql-query-builder@shasoft.com"
        }
